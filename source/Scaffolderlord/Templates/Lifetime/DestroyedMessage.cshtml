@using RazorLight
@using Scaffolderlord.Models.Lifetime
@inherits TemplatePage<DestroyedMessageTemplateModel>
using Common.Messaging;
@foreach (var item in Model.Usings)
{
	<text>using @item;</text>
}

namespace @Model.Namespace

@Model.XmlDoc($"An event that is published internally when a {Model.TypeName} is destroyed.")
internal class @(Model.TypeName)Destroyed : IEvent
{
	public @(Model.TypeName)Destroyed(@(Model.TypeName) instance)
	{
		Instance = instance;
	}
	public @(Model.TypeName) Instance { get; }
}